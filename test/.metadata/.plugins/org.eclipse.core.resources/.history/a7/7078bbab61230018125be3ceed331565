class Cleanser {
	private String s = "Cleanser";
	public void append(String a) {
		s+=a;
	}
	public void dilute() {
		append(" dilute() ");
	}
	public void apply() {
		append(" apply() ");
	}
	public void scrub() {
		append(" scurb() ");
	}
}

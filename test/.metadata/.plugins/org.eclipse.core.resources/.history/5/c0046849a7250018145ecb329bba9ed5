class Shap {
	public Shap(int i) {
		Print.print("Shap constructor");
	}

	void dispose() {
		Print.print("Shap dispose");
	}
}

class Circle extends Shap {

	public Circle(int i) {
		super(i);
		Print.print("Drawing Circle");
	}

	@Override
	void dispose() {
		Print.print("Erasing Circle");
		super.dispose();
	}

}
class Line extends Shap{

	public Line(int i) {
		super(i);
		// TODO Auto-generated constructor stub
	}
	
}

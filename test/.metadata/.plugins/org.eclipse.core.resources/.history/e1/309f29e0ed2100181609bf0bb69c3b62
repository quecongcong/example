public class SimpleNotes {
	Notes notes;
	public SimpleNotes(Notes notes) {
		this.notes = notes;
	}
	
	public void descripe(){
		System.out.print("这是一张:");
		switch (notes) {
		case ONE:
			System.out.println("一元纸币");
			break;
		case FIVE:
			System.out.println("一元纸币");
			break;
		case TEN:
			System.out.println("一元纸币");
			break;
		case TWENTY:
			System.out.println("一元纸币");
			break;
		case FIFTY:
			System.out.println("一元纸币");
			break;
		case HUNDRED:
			System.out.println("一元纸币");
			break;
		default:
			break;
		}
	}
}

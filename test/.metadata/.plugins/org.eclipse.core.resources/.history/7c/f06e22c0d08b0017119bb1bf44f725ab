public class Test {
	
	public  void test() {
		ClassLoader classLoader = this.getClass().getClassLoader();
		while(classLoader != null){
			System.out.println(classLoader.getClass().getCanonicalName());
			classLoader = classLoader.getParent();
		}
	}
	
	public static void main(String[] args) {
		
	}
}
